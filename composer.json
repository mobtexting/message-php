{
    "name": "mobtexting/message-sdk",
    "description": "A boilerplate for contributions.",
    "homepage": "https://github.com/mobtexting/message-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Sankar Suda",
            "email": "sankar@mobtexting.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "mobtexting/php-http-client": "1.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Mobtexting\\Message\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mobtexting\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev"
}